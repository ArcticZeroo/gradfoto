{
  "compilerOptions": {
    "module": "es2020",
    "target": "es6",
    "sourceMap": true,
    "outDir": "dist",
    "lib": [
      "dom",
      "dom.iterable",
      "es2016"
    ],
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src"
  ]
}